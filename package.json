{
  "name": "henry-steinhauer-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:fix": "eslint --ext \".ts,.vue\" --fix --ignore-path .gitignore .",
    "format": "prettier \"**/*.{ts,json,css,scss,vue,html,md}\" --check",
    "format:fix": "prettier \"**/*.{ts,json,css,scss,vue,html,md}\" --check --write",
    "commit": "git-cz",
    "coverage:badge": "node lib/index.js --config './.conversion-badge-config'"
  },
  "author": "Henry Steinhauer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/H3nSte1n/henrysteinhauer-website"
  },
  "homepage": "https://github.com/H3nSte1n/henrysteinhauer-website#readme",
  "lint-staged": {
    "*.ts": "npm lint",
    "*.vue": "npm lint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/sitemap": "^2.4.0",
    "core-js": "3.14.0",
    "nuxt": "^2.15.7",
    "nuxt-property-decorator": "2.9.1",
    "sass": "1.35.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "12.1.4",
    "@nuxt/types": "2.15.6",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/color-mode": "2.0.9",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@vue/test-utils": "1.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "commitizen": "4.2.4",
    "coverage-badge-creator": "1.0.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-vue": "7.10.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
