{
  "name": "henry-steinhauer-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:fix": "eslint --ext \".ts,.vue\" --fix --ignore-path .gitignore .",
    "format": "prettier \"**/*.{ts,json,css,scss,vue,html,md}\" --check",
    "format:fix": "prettier \"**/*.{ts,json,css,scss,vue,html,md}\" --check --write",
    "commit": "git-cz",
    "coverage:badge": "node lib/index.js --config './.conversion-badge-config'"
  },
  "author": "Henry Steinhauer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/H3nSte1n/henrysteinhauer-website"
  },
  "homepage": "https://github.com/H3nSte1n/henrysteinhauer-website#readme",
  "dependencies": {
    "@nuxtjs/sitemap": "2.4.0",
    "core-js": "3.21.1",
    "express": "4.17.2",
    "node-fetch": "2.6.7",
    "nuxt": "2.15.8",
    "nuxt-property-decorator": "2.9.1",
    "redirect-ssl": "3.0.0",
    "sass": "1.49.7"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "12.1.4",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/color-mode": "2.1.1",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@vue/test-utils": "1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "commitizen": "4.2.4",
    "coverage-badge-creator": "1.0.9",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-vue": "7.20.0",
    "prettier": "2.5.1"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
