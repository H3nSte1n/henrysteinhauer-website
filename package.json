{
  "name": "henry-steinhauer-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "format": "prettier \"**/*.{js,json,css,scss,vue,html,md}\" --check",
    "format:fix": "prettier \"**/*.{js,json,css,scss,vue,html,md}\" --check --write",
    "test": "jest --colors --coverage --config jest.config.js",
    "test:dev": "jest --watch --colors --verbose --config jest.config.js",
    "commit": "git-cz",
    "coverage:badge": "node lib/index.js --config './.conversion-badge-config'"
  },
  "author": "Henry Steinhauer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/H3nSte1n/henrysteinhauer-website"
  },
  "homepage": "https://github.com/H3nSte1n/henrysteinhauer-website#readme",
  "lint-staged": {
    "*.ts": "npm lint",
    "*.vue": "npm lint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && lint-staged"
    }
  },
  "dependencies": {
    "core-js": "3.12.1",
    "nuxt": "2.15.4",
    "nuxt-property-decorator": "2.9.1",
    "sass": "1.32.12"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "12.1.1",
    "@nuxt/types": "2.15.4",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/color-mode": "2.0.5",
    "@nuxtjs/eslint-config-typescript": "6.0.0",
    "@nuxtjs/eslint-module": "3.0.2",
    "@vue/test-utils": "1.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "commitizen": "4.2.4",
    "coverage-badge-creator": "1.0.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.2.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-vue": "7.9.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "11.0.0",
    "prettier": "2.2.1",
    "ts-jest": "26.5.6",
    "vue-jest": "3.0.7"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  }
}
